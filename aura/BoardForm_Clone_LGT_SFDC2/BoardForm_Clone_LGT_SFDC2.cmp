<aura:component implements="force:lightningQuickActionWithoutHeader,flexipage:availableForRecordHome,force:hasRecordId,lightning:actionOverride">
	
	<aura:attribute name="oldFormRecord" type="Boarding_Form__c" />
    <aura:attribute name="newFormRecord" type="Boarding_Form__c" />
	<aura:attribute name="newBoardingForm" type="Boarding_Form__c" default="{ 'sobjectType' : 'Boarding_Form__c' }" />
	<aura:attribute name="oldBoardingForm" type="Boarding_Form__c" default="{ 'sobjectType' : 'Boarding_Form__c' }" />
	<aura:attribute name="formRecordError" type="String" />

	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />

	<force:recordData aura:id="formRecordLoader"
					  recordId="{!v.recordId}"
					  layoutType="FULL"
                      mode="EDIT"
					  targetRecord="{!v.oldFormRecord}"
					  targetFields="{!v.oldBoardingForm}"
					  targetError="{!v.formRecordError}" />
    
    <force:recordData aura:id="boardingFormRecordCreator"
					  layoutType="FULL"
					  targetRecord="{!v.newFormRecord}"
					  targetFields="{!v.newBoardingForm}"
					  targetError="{!v.formRecordError}" />
    
    <div class="slds-page-header">
        <div class="slds-media">
            <div class="slds-media__figure">
                <lightning:icon iconName="custom:custom67" />
            </div>
            <div class="slds-media__body">
                <h3 class="slds-text-body_small slds-line-height_reset">Cloning Form</h3>
                <h1 class="slds-page-header__title slds-truncate slds-align-middle" 
                    title="{!v.newBoardingForm.Name}">{!v.newBoardingForm.Name}</h1>
            </div>
        </div>
    </div>
    
    <aura:if isTrue="{!not(empty(v.formRecordError))}">
        <ui:message title="Error" severity="error">
            {!v.formRecordError}
        </ui:message>
    </aura:if>
    
    <br />
    <div class="slds-form--stacked slds-p-around--medium slds-m-bottom--x-small">
        <table>
            <tr>
                <td>
                    <label>Boarding Form Name</label>
                    <br />
                    <force:outputField value="{!v.oldBoardingForm.Name}" />
                </td>
                <td>
                </td>
                <td>
                    <label>BF Status</label>
                    <force:inputField aura:id="BF-Status" value="{!v.oldBoardingForm.BF_Status__c}" change="{!c.handleChangeBFStatus}" />
                </td>
            </tr>
            <tr>
                <td>
                    <label>*Related LOQ</label>
                    <force:inputField aura:id="Related-LOQ" value="{!v.oldBoardingForm.Related_LOQ__c}" change="{!v.handleChangeRelatedLOQ}" />
                </td>
                <td>
                </td>
                <td>
                    <label>Opportunity</label>
                    <force:inputField aura:id="Opportunity" value="{!v.oldBoardingForm.Opportunity__c}" change="{!c.handleChangeOpp}" />
                </td>
            </tr>
        </table>
    </div>
