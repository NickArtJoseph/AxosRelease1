    <validationRules>
        <fullName>LLC_BI__Contingent_Amount_Exceeds_Loan_Amount</fullName>
        <active>true</active>
        <description>Ensures the contingent amount does not exceed the loan amount.</description>
        <errorConditionFormula>LLC_BI__Contingent_Amount__c &gt; LLC_BI__Loan__r.LLC_BI__Amount__c</errorConditionFormula>
        <errorDisplayField>LLC_BI__Contingent_Amount__c</errorDisplayField>
        <errorMessage>Contingent amount cannot exceed the loan amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__Contingent_Amount_Less_Than_0</fullName>
        <active>true</active>
        <description>Contingent Amount a negative value.</description>
        <errorConditionFormula>LLC_BI__Contingent_Amount__c &lt; 0</errorConditionFormula>
        <errorMessage>Contingent Amount must be greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__Contingent_Amount_and_Contingent_Percent</fullName>
        <active>true</active>
        <description>Only one field can be used to calculate contingent guarantee. Please specify either amount or percentage before saving.</description>
        <errorConditionFormula>( LLC_BI__Contingent_Amount__c &gt; 0 &amp;&amp;  LLC_BI__Ownership__c &gt; 0) &amp;&amp;
(!CONTAINS(TEXT(LLC_BI__Borrower_Type__c) , &apos;Household&apos;))</errorConditionFormula>
        <errorMessage>Only one field can be used to calculate contingent guarantee. Please specify either amount or percentage before saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__HMDA_Demographic</fullName>
        <active>false</active>
        <description>--DEPRECATED--</description>
        <errorConditionFormula>FALSE</errorConditionFormula>
        <errorMessage>--DEPRECATED--</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__Ownership_Less_Than_0</fullName>
        <active>true</active>
        <description>Contingent Percentage must be greater than 0.</description>
        <errorConditionFormula>LLC_BI__Ownership__c &lt; 0</errorConditionFormula>
        <errorMessage>Contingent Percentage must be greater than 0.</errorMessage>
    </validationRules>
