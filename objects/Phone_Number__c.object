    <webLinks>
        <fullName>Order_Neustar_Phone_Attributes</fullName>
        <availability>online</availability>
        <description>Triggers Neustar callout to retrieve Phone Attribute data</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Order Neustar Phone Attributes</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)} 
var outputMessage = { 
title: &apos;&apos;, 
message: &apos;&apos; 
};
var continueProcessing = true;
var profileName = &apos;{!$Profile.Name}&apos;.replace(/\s+/g,&apos; &apos;).trim();
var authorizedUser = &apos;Bradley Greer&apos;;
var userName = &apos;{!User.Name}&apos;;

if(!(userName == authorizedUser ||
profileName == &apos;System Administrator&apos;)) {
outputMessage.title = &apos;PERMISSION ISSUE&apos;;
outputMessage.message = &apos;You do not have proper permissions to manually trigger a Neustar callout. Please contact &apos; + authorizedUser + &apos; or create an IT Support ticket for further assistance.&apos;;
continueProcessing = false;
} else {
continueProcessing = confirm(&apos;This process will initiate an order of Neustar Phone Attribute data for this Phone Number record. Please confirm that you wish to continue with this process.&apos;);
if(!continueProcessing) {
outputMessage.title = &apos;CANCELLED&apos;;
outputMessage.message = &apos;No Neustar Phone Attribute Order initiated.&apos;;
}
}
if(continueProcessing) {
var phoneArray = [];
var phone = new sforce.SObject(&quot;Phone_Number__c&quot;);
phone.Id = &apos;{!Phone_Number__c.Id}&apos;;
phone.Neustar_Trigger__c = true;
phoneArray[0] = phone;

var errorMsg;
var result = sforce.connection.update(phoneArray);
for(var i = 0; i &lt; result.length; i++) {
  if(result[i].success != &apos;true&apos;) {
    errorMsg = result[i].errors.message;
  }
}

if(errorMsg == null) {
  outputMessage.title = &apos;SUCCESS&apos;;
  outputMessage.message = &apos;You successfully initiated a Neustar Phone Attribute order. &lt;br/&gt;&lt;/br&gt;[NOTE: Results may not be immediately available so be judicious with triggering multiple times in succession.]&apos;;
} else {
  outputMessage.title = &apos;FAILURE&apos;;
  outputMessage.message = &apos;An error occurred while attempting to initiate the Neustar Phone Attribute order.&lt;br/&gt;&lt;ul&gt;&lt;li&gt;Error: &apos; + errorMsg + &apos;&lt;/li&gt;&lt;/ul&gt;&apos;;
}
}

var box = new SimpleDialog(&quot;hersh&quot;+Math.random(), true); 
parent.box = box; 
box.setTitle(outputMessage.title); 
box.createDialog(); 
box.setWidth(350); 
box.setContentInnerHTML(outputMessage.message + &apos;&lt;p&gt;&lt;div style=&quot;width: 100%; text-align: center;&quot;&gt;&lt;button class=&quot;btn&quot; onclick=&quot;window.parent.box.hide();window.parent.location.href=window.parent.location.href;return false;&quot;&gt;Close&lt;/button&gt;&lt;/div&gt;&lt;/p&gt;&apos;); 
box.setupDefaultButtons(); 
box.show();</url>
