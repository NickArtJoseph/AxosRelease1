    <validationRules>
        <fullName>Approved</fullName>
        <active>true</active>
        <description>Restrict field edits when Status is LOQ Approved. Only Julie can update a record</description>
        <errorConditionFormula>AND(
 TEXT(LOQ_Status__c) = &quot;LOQ Approved&quot;,
 NOT(
  OR(
   $Profile.Name = &quot;System Administrator&quot;, 
   $Profile.Name = &quot;API Service Account&quot;, 
   $UserRole.Name = &quot;C&amp;I Manager - Loan Operations&quot; 
  )
 ),
 OR(
  ISCHANGED(Opportunity__c),
  ISCHANGED(C_I_Loan_Type__c),
  ISCHANGED(Purpose_Code__c),
  ISCHANGED(Collateral_Code_Description__c),
  ISCHANGED(Call_Report_Code__c),   
  ISCHANGED(Collateral_Type__c),
  ISCHANGED(Collateral_Secured__c),
  ISCHANGED(Property_Type__c),
  ISCHANGED(Collateral_Type_RE__c),
  ISCHANGED(Borrower_Relationship__c),
  ISCHANGED(Term_Type__c),
  ISCHANGED(Interest_Rate_Type__c),
  ISCHANGED(Addl_LOQ_Options__c)
 )
)</errorConditionFormula>
        <errorMessage>Your profile is not able to make changes to the LOQ once it has been marked as Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LOQ_Loan_Ops_Only_can_edit_top_section</fullName>
        <active>true</active>
        <description>Only Julie can edit that section</description>
        <errorConditionFormula>AND(
NOT(
OR(
$Profile.Name = &quot;System Administrator&quot;,
$Profile.Name = &quot;API Service Account&quot;,
$UserRole.Name = &quot;C&amp;I Manager - Loan Operations&quot;
)
),
OR(
ISCHANGED(C_I_Loan_Type__c),
ISCHANGED(Purpose_Code__c),
ISCHANGED(Collateral_Code_Description__c ),
ISCHANGED(Call_Report_Code__c )
)
)</errorConditionFormula>
        <errorMessage>Only Loan Operations can fill in the top section of the LOQ.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LOQ_PreApproval_Check</fullName>
        <active>true</active>
        <description>Checks to make sure all necessary fields are filled in</description>
        <errorConditionFormula>AND(
NOT( 
OR( 
$Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;API Service Account&quot;, 
$UserRole.Name = &quot;C&amp;I Manager - Loan Operations&quot; 
) 
), 
OR(
ISPICKVAL(Collateral_Type__c,&quot;&quot; ),
ISPICKVAL( Term_Type__c ,&quot;&quot; ),
ISPICKVAL( Interest_Rate_Type__c ,&quot;&quot; ),
ISPICKVAL( Borrower_Relationship__c ,&quot;&quot; ),
ISPICKVAL( Collateral_Secured__c ,&quot;&quot; ),
ISPICKVAL( Property_Type__c ,&quot;&quot; ),
ISPICKVAL( Collateral_Type_RE__c ,&quot;&quot; ),
ISBLANK(Addl_LOQ_Options__c)
)
,
OR( 
ISPICKVAL(LOQ_Status__c, &quot;LOQ Submitted for approval&quot;),
ISPICKVAL(LOQ_Status__c, &quot;LOQ Approved&quot;)
)
)</errorConditionFormula>
        <errorMessage>All 8 LOQ questions must be answered before submitting to Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Submitted_for_Approval</fullName>
