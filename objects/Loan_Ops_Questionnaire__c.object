  continueProcessing = false;
}

if(continueProcessing) {
  window.open(&apos;https://composer.congamerge.com?sessionId={!API.Session_ID}&amp;serverUrl={!API.Partner_Server_URL_80}&amp;id={!Loan_Ops_Questionnaire__c.Id}&amp;DefaultPDF=1&amp;SC0=1&amp;SC1=Attachments&amp;TemplateGroup=CI_LOQ&amp;BML={!Loan_Ops_Questionnaire__c.Name + &quot; Boarding Form Questionnaire Creation&quot;}&amp;QueryId=a0ui0000009sY1x&amp;OFN={!Loan_Ops_Questionnaire__c.Name}+Boarding+Form+Questionnaire&amp;TemplateId=a0Ni000001wxUqt&apos;,&apos;_blank&apos;, &apos;toolbar=0,location=0,menubar=0&apos;);
} else {
  var box = new SimpleDialog(&quot;hersh&quot;+Math.random(), true); 
