        <fullName>Next_Stage</fullName>
        <availability>online</availability>
        <description>Button to create Boarding Form if all reqs are met</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Next Stage</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)} 
var outputMessage = { 
title: &apos;&apos;, 
message: &apos;&apos; 
};
var continueProcessing = true;

if(&apos;{!Loan_Ops_Questionnaire__c.LOQ_Status__c}&apos; != &apos;LOQ Approved&apos;) {
  outputMessage.title = &apos;STATUS ISSUE&apos;;
  outputMessage.message = &apos;The current LOQ is not yet approved. You may not progress to the Next Stage.&apos;;
  continueProcessing = false;
}

if(continueProcessing) {
  var bfURL = &apos;/{!$ObjectType.Boarding_Form__c}/e?retURL={!Loan_Ops_Questionnaire__c.Id}&apos;;

  //IMPORTANT: FIELD NAMES WILL CHANGE AFTER PUSHED TO NEW INSTANCE
  var result = sforce.connection.query(&quot;select Id, Name, CI_Tax_ID__c, CI_Loan_Number__c, CI_Risk_Rating__c, CI_NAICS_Index__c, CI_Loan_Amount__c, CI_Closing_Fee_s__c, CI_Projected_Initial_Funding__c, CI_Rate__c, CI_Primary_Analyst__c, CI_Primary_Analyst__r.Name, OwnerId, Owner.Name, CI_Borrower_Legal_Name__r.Name, CI_Borrower_Legal_Name__r.Id, CI_Borrower_Legal_Name__r.Phone, CI_Borrower_Legal_Name__r.BillingStreet, CI_Borrower_Legal_Name__r.BillingCity, CI_Borrower_Legal_Name__r.BillingState, CI_Borrower_Legal_Name__r.BillingPostalCode, CI_Borrower_Legal_Name__r.BillingCountry from Opportunity where Id = &apos;{!Loan_Ops_Questionnaire__c.OpportunityId__c}&apos;&quot;);
  var opp = result.getArray(&quot;records&quot;)[0];
  var oppOwner = opp.getArray(&quot;Owner&quot;)[0];
  var primAn = opp.getArray(&quot;CI_Primary_Analyst__r&quot;)[0];
  var bwr = opp.getArray(&quot;CI_Borrower_Legal_Name__r&quot;)[0];

  bfURL += &apos;&amp;CF00Ni000000HqyJz_lkid=&apos; + opp.Id; //Opportunity ID
  bfURL += &apos;&amp;CF00Ni000000HqyJz=&apos; + escape(opp.Name); //Opportunity Name
  bfURL += &apos;&amp;CF00Ni000000HqyJy_lkid={!Loan_Ops_Questionnaire__c.Id}&apos;; //Related LOQ ID
  bfURL += &apos;&amp;CF00Ni000000HqyJy=&apos; + escape(&apos;{!Loan_Ops_Questionnaire__c.Name}&apos;); //Related LOQ Name
  bfURL += &apos;&amp;00Ni000000HqyKu=&apos; + escape(&apos;{!Loan_Ops_Questionnaire__c.C_I_Loan_Type__c}&apos;); //CI Loan Type
  bfURL += &apos;&amp;00Ni000000HqyO5=&apos; + escape(&apos;{!Loan_Ops_Questionnaire__c.Purpose_Code__c}&apos;); //Purpose Code
  bfURL += &apos;&amp;00Ni000000HqyKz=&apos; + escape(&apos;{!Loan_Ops_Questionnaire__c.Collateral_Code_Description__c}&apos;); //Collateral Code Description
  if(bwr != null) {
    bfURL += &apos;&amp;00Ni000000I2zi0=&apos; + escape(bwr.Name || &apos;&apos;); //BWR Legal Name
    bfURL += &apos;&amp;00Ni000000HqyKl=&apos; + escape(bwr.BillingStreet || &apos;&apos;); //Borrower Mailing Address Street
    bfURL += &apos;&amp;00Ni000000HqyKh=&apos; + escape(bwr.BillingCity || &apos;&apos;); //Borrower Mailing Address City
    bfURL += &apos;&amp;00Ni000000HqyKk=&apos; + escape(bwr.BillingState || &apos;&apos;); //Borrower Mailing Address State
    bfURL += &apos;&amp;00Ni000000HqyKj=&apos; + escape(bwr.BillingPostalCode || &apos;&apos;); //Borrower Mailing Address Postal Code
    bfURL += &apos;&amp;00Ni000000HqyKi=&apos; + escape(bwr.BillingCountry || &apos;&apos;); //Borrower Mailing Address Country
    bfURL += &apos;&amp;00Ni000000HqyKm=&apos; + escape(bwr.Phone || &apos;&apos;); //Borrower Phone Number
  }
  bfURL += &apos;&amp;00Ni000000HqyKo=&apos;+ escape(opp.CI_Tax_ID__c || &apos;&apos;); //Borrower Tax ID Number
  bfURL += &apos;&amp;00Ni000000HqyNG=&apos;+ escape(opp.CI_Loan_Number__c || &apos;&apos;); //Loan Number
  bfURL += &apos;&amp;00Ni000000HqyOQ=&apos;+ escape(opp.CI_Risk_Rating__c || &apos;&apos;); //Risk Rating
  bfURL += &apos;&amp;CF00Ni000000HqyOO_lkid=&apos; + opp.OwnerId; //Relationship Manager ID
  bfURL += &apos;&amp;CF00Ni000000HqyOO=&apos; + escape(oppOwner.Name); //Relationship Manager Name
  bfURL += &apos;&amp;00Ni000000HqyNO=&apos; +  escape(opp.CI_NAICS_Index__c || &apos;&apos;); //NAICS Code
  bfURL += &apos;&amp;00Ni000000HqyNC=&apos; + escape(opp.CI_Loan_Amount__c || &apos;&apos;); //Loan Amount
  bfURL += &apos;&amp;00Ni000000HqyNZ=&apos; + escape(opp.CI_Closing_Fee_s__c || &apos;&apos;); //Origination Fee Discount
  bfURL += &apos;&amp;00Ni000000HqyNt=&apos; + escape(opp.CI_Projected_Initial_Funding__c || &apos;&apos;); //Projected Initial Funding
  bfURL += &apos;&amp;00Ni000000HqyOL=&apos; + escape(opp.CI_Rate__c || &apos;&apos;); //Rate
  bfURL += &apos;&amp;00Ni000000HqyKw=&apos; + escape(&apos;{!Loan_Ops_Questionnaire__c.Call_Report_Code__c}&apos;); //Call Report Code
  if(primAn != null) {
    bfURL += &apos;&amp;CF00Ni000000HqyNr_lkid=&apos; + opp.CI_Primary_Analyst__c; //Primary Analyst ID
    bfURL += &apos;&amp;CF00Ni000000HqyNr=&apos; + escape(primAn.Name); //Primary Analyst Name
  }

  window.open(bfURL, &apos;_top&apos;);
} else {
  var box = new SimpleDialog(&quot;hersh&quot;+Math.random(), true); 
  parent.box = box; 
  box.setTitle(outputMessage.title); 
  box.createDialog(); 
  box.setWidth(350); 
  box.setContentInnerHTML(outputMessage.message + &apos;&lt;p&gt;&lt;div style=&quot;width: 100%; text-align: center;&quot;&gt;&lt;button class=&quot;btn&quot; onclick=&quot;window.parent.box.hide();window.parent.location.href=window.parent.location.href;return false;&quot;&gt;Close&lt;/button&gt;&lt;/div&gt;&lt;/p&gt;&apos;); 
  box.setupDefaultButtons(); 
  box.show();
}</url>
