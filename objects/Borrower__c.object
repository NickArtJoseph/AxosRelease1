    <validationRules>
        <fullName>Borrower_Phone_Number_Format</fullName>
        <active>true</active>
        <description>Enforces 10 digits, no characters. Allows parentheses and dash for the data loader.</description>
        <errorConditionFormula>NOT(
OR(
ISBLANK(Phone_Number__c ),
REGEX( Phone_Number__c  , &quot;\\D*?(\\d\\D*?){10}&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>Phone_Number__c</errorDisplayField>
        <errorMessage>Verify the phone number is in the format: (999) 999-9999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Borrower_Postal_Code_Numeric</fullName>
        <active>true</active>
        <description>Validates that the BWR Postal Code field only contains numeric values</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Mailing_Address_Postal_Code__c)),
NOT(ISNUMBER(Mailing_Address_Postal_Code__c))
)</errorConditionFormula>
        <errorDisplayField>Mailing_Address_Postal_Code__c</errorDisplayField>
        <errorMessage>Only numeric characters are allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Borrower_State_Alphabetic</fullName>
        <active>true</active>
        <description>Validates that the BWR Mailing State field only contains letters</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Mailing_Address_State__c)),
NOT(REGEX(Mailing_Address_State__c, &quot;[a-zA-Z]*(\\s[a-zA-Z]*)?&quot;))
)</errorConditionFormula>
        <errorDisplayField>Mailing_Address_State__c</errorDisplayField>
        <errorMessage>Only letters and a single space are allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Borrower_Tax_ID_Number_TIN_Format</fullName>
        <active>true</active>
        <description>Requires formats: 
 &quot;XXX-XX-XXXX&quot; where X is digit 0-9</description>
        <errorConditionFormula>NOT(
    OR( ISBLANK( Tax_ID_Number__c ),
        REGEX( Tax_ID_Number__c , &quot;[\\d]{3}-[\\d]{2}-[\\d]{4}&quot; ),
        REGEX( Tax_ID_Number__c , &quot;[\\d]{2}-[\\d]{7}&quot; )
      )
)</errorConditionFormula>
        <errorDisplayField>Tax_ID_Number__c</errorDisplayField>
        <errorMessage>Borrower Tax ID Number must be in format  &quot;XXX-XX-XXXX&quot; or &quot;XX-XXXXXXX&quot; including the dash(es), where X is a 0-9 digit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SFDC2_Borrower_RecordLocked</fullName>
        <active>false</active>
        <description>The User can&apos;t change the valoues of this record to avoid modifications in the Approval Flow</description>
        <errorConditionFormula>AND(
      $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
      OR(  ISPICKVAL(Boarding_Form__r.BF_Status__c, &quot;BF Submitted for Pre-Approval&quot;) ,
           ISPICKVAL(Boarding_Form__r.BF_Status__c, &quot;BF Submitted for Approval&quot;) ,
           ISPICKVAL(Boarding_Form__r.BF_Status__c, &quot;BF Approved&quot;) 

        ),
      RecordReady__c     
)</errorConditionFormula>
        <errorMessage>You can not modify this record because the related BF is pending to be approved or approved, contact your administrator.</errorMessage>
    </validationRules>
