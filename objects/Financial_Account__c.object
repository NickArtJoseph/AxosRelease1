    <webLinks>
        <fullName>Retention_Referral</fullName>
        <availability>online</availability>
        <description>Sets up a new opportunity form with prepopulated values based on the current Financial Account record</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Retention Referral</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}

var outputMessage = { 
title: &apos;&apos;, 
message: &apos;&apos; 
};
var continueProcessing = true;

//lookup associated Accounts
var perFinJunc = sforce.connection.query(&quot;SELECT Person_Account__c, Person_Account__r.Name FROM Person_Financial_Account_Junction__c WHERE Financial_Account__c = &apos;{!Financial_Account__c.Id}&apos;&quot;);
var records = perFinJunc.getArray(&quot;records&quot;);

if(records.length == 0) {
  outputMessage.title = &quot;NO ASSOCIATED ACCOUNTS&quot;;
  outputMessage.message = &quot;This financial account record is not associated with any person accounts. You can not create a retention referral opportunity at this time.&quot;;
  continueProcessing = false;
}

if(continueProcessing) {
  var accountOptions = &quot;&quot;;
  for (var i = 0; i &lt; records.length; i++) {
    accountOptions += &quot;&lt;option value=&apos;&quot; + records[i].Person_Account__c + &quot;&apos;&gt;&quot; + records[i].Person_Account__r.Name + &quot;&lt;/option&gt;&quot;;
  }
  outputMessage.title = &quot;SELECT ACCOUNT&quot;;
  outputMessage.message = &quot;Please select the appropriate person account:&amp;nbsp;&quot; +
    &quot;&lt;select id=&apos;RetRef_AccountSelect&apos; size=&apos;1&apos;&gt;&quot; + accountOptions + 
    &quot;&lt;/select&gt;&quot;;
}

var box = new SimpleDialog(&quot;hersh&quot;+Math.random(), true); 
parent.box = box; 
box.setTitle(outputMessage.title); 
box.createDialog(); 
box.setWidth(350); 
box.setContentInnerHTML(outputMessage.message + &apos;&lt;p&gt;&lt;div style=&quot;width: 100%; text-align: center;&quot;&gt;&lt;button class=&quot;btn&quot; onclick=&quot;window.parent.box.hide();window.parent.location.href=\&apos;/006/e?opp3=Retention+Referral+{!Financial_Account__c.Brand__c}&amp;opp6=Existing+Relationship&amp;opp11=New+Assigned&amp;opp9={!TODAY()}&amp;opp7={!Financial_Account__c.Current_Balance__c}&amp;CF00Ni000000FixAU={!Financial_Account__c.Name}&amp;CF00Ni000000FixAU_lkid={!Financial_Account__c.Id}&amp;00Ni000000FixAT=Retention+Referral&amp;retURL=%2F{!Financial_Account__c.Id}&amp;RecordType=012i0000001AbiI&amp;ent=Opportunity&amp;accid=\&apos; + document.getElementById(\&apos;RetRef_AccountSelect\&apos;).options[document.getElementById(\&apos;RetRef_AccountSelect\&apos;).selectedIndex].value;return false&quot;&gt;Create&lt;/button&gt;&lt;button class=&quot;btn&quot; onclick=&quot;window.parent.box.hide();alert(1);return false;&quot;&gt;Close&lt;/button&gt;&lt;/div&gt;&lt;/p&gt;&apos;); 
box.setupDefaultButtons(); 
box.show();</url>
