    <validationRules>
        <fullName>Document_Other_SFDC2</fullName>
        <active>true</active>
        <description>When the Document is set to &quot;None,&quot; the Document Other field must be populated.</description>
        <errorConditionFormula>AND(ISPICKVAL( Document__c , &quot;Other&quot;),
ISBLANK( Document_Other_SFDC2__c ))</errorConditionFormula>
        <errorDisplayField>Document_Other_SFDC2__c</errorDisplayField>
        <errorMessage>Please enter a value in the Document Other field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rollback_Stages</fullName>
        <active>true</active>
        <description>Non-Managers (C&amp;I - LF &amp; CRESL Role) cannot move Document Statuses backwards</description>
        <errorConditionFormula>OR(
    AND(  
         $UserRole.DeveloperName = &quot;C_I_LF_CRESL&quot;,
         ISCHANGED(Status__c), 
          
         VALUE(LEFT(TEXT(PRIORVALUE(Status__c)),1)) &gt; 
         VALUE(LEFT(TEXT(Status__c),1)) 
       ),
    AND(
             PRIORVALUE(Status__c) &lt;&gt; null,
             ISBLANK(TEXT(Status__c))   
     )

)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Non-Managers cannot move Document Statuses backwards</errorMessage>
    </validationRules>
