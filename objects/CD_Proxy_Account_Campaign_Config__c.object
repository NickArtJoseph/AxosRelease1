<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Configuration settings for the Consumer Deposits Proxy Account Campaign Creation functionality</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Campaign_Owner_Id__c</fullName>
        <description>Default owner id for campaigns created</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter the User Id of the default campaign owner. If left blank, the user triggering the process will become the owner.</inlineHelpText>
        <label>Campaign Owner Id</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Campaign_Record_Type_Id__c</fullName>
        <description>The campaign record type for records created with this process</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter the record type id to be used for Campaigns created with this process.</inlineHelpText>
        <label>Campaign Record Type Id</label>
        <length>20</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Report_Id_List__c</fullName>
        <description>Comma-delimited list of report ids to be used as the source for campaign creation</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter the report id(s) to be used to generate the campaigns/campaign members. Separate each report id with a comma.</inlineHelpText>
        <label>Report Id List</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>CD - Proxy Account Campaign Config</label>
    <visibility>Protected</visibility>
</CustomObject>
